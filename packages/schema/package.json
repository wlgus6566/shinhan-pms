{
  "name": "@repo/schema",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "default": "./dist/auth/index.js"
    },
    "./projects": {
      "types": "./dist/projects/index.d.ts",
      "default": "./dist/projects/index.js"
    },
    "./tasks": {
      "types": "./dist/tasks/index.d.ts",
      "default": "./dist/tasks/index.js"
    },
    "./work-logs": {
      "types": "./dist/work-logs/index.d.ts",
      "default": "./dist/work-logs/index.js"
    },
    "./users": {
      "types": "./dist/users/index.d.ts",
      "default": "./dist/users/index.js"
    },
    "./schedules": {
      "types": "./dist/schedules/index.d.ts",
      "default": "./dist/schedules/index.js"
    },
    "./common": {
      "types": "./dist/common/index.d.ts",
      "default": "./dist/common/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "typescript": "^5.3.3"
  }
